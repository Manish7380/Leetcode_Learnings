class Solution {
    public int minOperations(int[] nums, int k) {

        int sum = 0;

        // Step 1: Calculate total sum of array
        for (int i = 0; i < nums.length; i++) {
            sum = sum + nums[i];
        }

        // Step 2: Return remainder
        int result = sum % k;

        return result;
    }
}
